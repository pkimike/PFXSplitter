<Window x:Class="PFXSplitter.Views.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:PFXSplitter.Views.UserControls"
        xmlns:dc="clr-namespace:PFXSplitter.ViewModels"
        xmlns:local="clr-namespace:PFXSplitter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <dc:MainWindowViewModel/>
    </Window.DataContext>
    <TabControl Background="{StaticResource PanelBackgroundBrush}">
        <TabItem Header="Split Pfx">
            <uc:FromPfxUserControl DataContext="{Binding FromPfx}"></uc:FromPfxUserControl>
        </TabItem>
    </TabControl>
</Window>
